{
  "name": "redux-ramda-playground",
  "version": "1.0.0",
  "description": "a space to bring more functional features into redux and react",
  "main": "index.js",
  "keywords": [
    "ramda",
    "redux",
    "functional",
    "react"
  ],
  "author": "Lucas Schejtman <lucas.schejtman@gmail.com>",
  "license": "ISC",
  "scripts": {
    "test": "ava",
    "coverage": "nyc --reporter=html npm test",
    "dev": "webpack-dev-server --config ./webpack/webpack.config.dev.js --hot --inline --progress"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": "5",
    "require": [
      "./test/helpers/setup-browser-env",
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "6.7.7",
    "babel-eslint": "6.0.3",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "enzyme": "^2.4.1",
    "file-loader": "0.8.5",
    "jsdom": "^9.4.1",
    "nyc": "^8.0.0",
    "proxyquire": "^1.7.10",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-slider-monitor": "1.0.5",
    "sinon": "^1.17.5",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "apollo-client": "^0.4.20",
    "glob": "^7.0.5",
    "graphql-tag": "^0.1.14",
    "history": "2.1.0",
    "material-ui": "^0.15.1",
    "radium": "^0.17.1",
    "ramda": "^0.21.0",
    "react": "15.0.1",
    "react-apollo": "^0.5.8",
    "react-dom": "15.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "3.5.2",
    "redux-actions": "^0.10.0",
    "redux-form": "^5.3.1",
    "redux-promise": "^0.5.3"
  }
